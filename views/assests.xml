<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="assets_backend" name="account assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/school/static/src/js/helloworld.js"/>
            <script type="text/javascript" src="/school/static/src/js/hr_attendance_new.js" />
            <script type="text/javascript" src="/school/static/src/js/sales_report.js" />
        </xpath>
    </template>

    <template id="assets_frontend" name="popup assets" inherit_id="web.assets_frontend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/school/static/src/js/popup.js"></script>
            <link rel="stylesheet" href="/school/static/src/css/home.css"/>
        </xpath>
    </template>

</odoo>
