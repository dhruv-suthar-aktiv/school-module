<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for student.student -->
        <record id="seq_student_student" model="ir.sequence">
            <field name="name">Student</field>
            <field name="code">student.student.sequence</field>
            <field name="prefix">ST</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for student.student -->
        <record id="seq_professor_professor" model="ir.sequence">
